import java.util.*;
class Solution{
    public int solution(int [][]board)    {
        int answer = 0;
        ArrayList<String>list = new ArrayList<>();

        for(int i=0;i< board.length;i++){
            String temp ="";
            for(int j=0;j<board[i].length;j++){
                temp+=String.valueOf(board[i][j]);
            }
            list.add(temp);
        }


        return answer;
    }
}

